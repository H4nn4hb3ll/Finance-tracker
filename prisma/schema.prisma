generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  userID        Int     @id @default(autoincrement())
  username      String  @unique
  password      String
  sessionStatus Boolean @default(false)
}

model transactions {
  transactionID Int      @id @default(autoincrement())
  date          DateTime
  category      String
  amount        Float
  type          String
}
